<Application
	x:Class="PluginDetectorForSc4pac.App"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	RequestedThemeVariant="Default">
	<!--  "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options.  -->

	<Application.Styles>
		<FluentTheme />
		<StyleInclude Source="Styles/DefaultStyles.axaml" />
	</Application.Styles>

	<Application.Resources>
		<FontFamily x:Key="Montserrat">avares://PluginDetectorForSc4pac/Assets/Fonts/Montserrat-Regular.ttf#Montserrat</FontFamily>
		<FontFamily x:Key="Phosphor">avares://PluginDetectorForSc4pac/Assets/Fonts/Phosphor.ttf#Phosphor</FontFamily>
		<FontFamily x:Key="PhosphorFill">avares://PluginDetectorForSc4pac/Assets/Fonts/Phosphor-Fill.ttf#Phosphor-Fill</FontFamily>
		<FontFamily x:Key="SegoeIcon">avares://PluginDetectorForSc4pac/Assets/Fonts/Segoe Fluent Icons.ttf#Segoe Fluent Icons</FontFamily>
	</Application.Resources>
</Application>
